{% extends "theme::layouts/default" %}

{% block content %}

    <div id="pyradic">
        <div class="container-fluid">
            <div class="card">
                <div class="card-block">
                    <h4 class="title">
                        {{ trans('module::addon.section.default_field_resolutions') }}
                    </h4>

                    <table class="table">
                        <thead>
                        <tr>
                            <th>Field Type</th>
                            <th>GraphQL Type</th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for field, type in default_field_resolutions %}
                            <tr>
                                <td>{{ field }}</td>
                                <td>{{ type }}</td>
                            </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
